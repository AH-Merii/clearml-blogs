     "    \"    colors = colors[: len(network_names)]\\n\",\n",
     "    \"    r_sqr_array = []\\n\",\n",
     "    \"    csv_file = pd.read_csv(csv_filepath)\\n\",\n",
     "    \"    t_train = np.array(csv_file[csv_file.columns[1]])\\n\",\n",
