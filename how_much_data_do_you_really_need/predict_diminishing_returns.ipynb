     "    \"    :return: Prediction of the model given (t;x)\\n\",\n",
     "    \"    \\\"\\\"\\\"\\n\",\n",
     "    \"    return np.log(eps + t * x[0]) * x[1] + np.log(np.log(eps + t * x[0])) * x[1]\\n\",\n",
     "    \"\\n\",\n",
     "    \"\\n\",\n",
     "    \"def get_train_sumples(t: np.ndarray, y:np.ndarray, indicies_array:np.ndarray) -> (np.ndarray, np.ndarray):\\n\",\n",
     "    \"    \\\"\\\"\\\"\\n\",\n",
